{
    "version": 2,
    "build": {
      "env": {
        "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true",
        "PUPPETEER_EXECUTABLE_PATH": "/usr/bin/chromium-browser"
      },
      "scripts": {
        "build": "npm run build",
        "install": "apt-get update && apt-get install -y chromium-browser"
      }
    },
    "builds": [
      { "src": "*js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/" }
    ]
    
}